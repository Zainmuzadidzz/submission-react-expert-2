{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress/globals": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", " cypress"],
  "rules": {
    "eol-last": "off"
  },
  "ignorePatterns": ["node_modules", "/public/**", "/resources/** "]
}
